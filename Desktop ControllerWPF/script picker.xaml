<Window x:Class="Desktop_ControllerWPF.script_picker"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Desktop_ControllerWPF"
        mc:Ignorable="d"
        Title="script_picker" Height="420" Width="420">
    <Grid>
        <DataGrid x:Name="scriptGrid" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="347" Width="394"/>
        <Button x:Name="Delete" Content="Delete" HorizontalAlignment="Left" Margin="329,362,0,0" VerticalAlignment="Top" Width="75" Click="Delete_Click"/>
        <Button x:Name="Ok" Content="Ok" HorizontalAlignment="Left" Margin="10,362,0,0" VerticalAlignment="Top" Width="75" Click="Ok_Click"/>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Left" Margin="101,362,0,0" VerticalAlignment="Top" Width="75" Click="Cancel_Click"/>
    </Grid>
</Window>
